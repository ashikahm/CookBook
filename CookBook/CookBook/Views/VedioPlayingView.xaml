<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CookBook.Views.VedioPlayingView"
             xmlns:local="clr-namespace:CookBook.ViewModels"
             xmlns:cmt="http://xamarin.com/schemas/2020/toolkit">
    <ContentPage.Content>
        <cmt:MediaElement x:Name="mediasource" AutoPlay="True" ShowsPlaybackControls="True" KeepScreenOn="True" HeightRequest="500" WidthRequest="300"/>
    </ContentPage.Content>
</ContentPage>